<p id="notice"><%= notice %></p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<p>
  <strong>Nombre:</strong>
  <%= @user.full_name %>
</p>
<% if @user == current_user %>
  <p>
    <strong>Token:</strong>
    <%= @user.token  %>
  </p>
<% end %>



<% if current_user%>
  <%= link_to 'Editar', edit_user_path(@user), :class=>"button-generic" %> |
<% end %>
<%= link_to 'Volver', users_path, :class=>"button-generic" %>
